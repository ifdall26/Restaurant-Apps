{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console":"off",
        "eol-last":"off",
        "no-multiple-empty-lines" :"off",
        "max-len":"off",
        "no-alert" :"off",
        "comma-dangle":"off",
        "object-shorthand":"off",
        "no-restricted-globals" :"off"
    }
}
